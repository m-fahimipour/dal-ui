    "eslint": "^9.19.0",
    "eslint-plugin-check-file": "^3.0.0",
    "eslint-plugin-perfectionist": "^4.8.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
